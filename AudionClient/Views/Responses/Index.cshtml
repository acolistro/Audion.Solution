@{
  Layout = "_Layout";
}

@using AudionClient.Models;

@using System.Security.Claims;
@* @model IEnumerable<AudionClient.Models.UserResponse>; *@


<h1> Responses </h1> @*random responses go on this page*@

@if (@Model.Count == 0)
{
  <h3>No response have been added yet!</h3>
} 
@foreach (UserResponse response in Model)
{
  <ul>
    <li>@Html.ActionLink($"{@response.Text}", "Details", "Responses", new { responseId = response.ResponseId})</li>
  </ul>
}

